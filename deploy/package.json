{
  "name": "flou-info-infrastructure",
  "version": "1.0.0",
  "description": "Infrastructure for Flou info landing page - S3, CloudFront",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --verbose --stage dev",
    "deploy:prod": "serverless deploy --verbose --stage prod",
    "remove": "serverless remove",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod",
    "info": "serverless info",
    "info:dev": "serverless info --stage dev",
    "info:prod": "serverless info --stage prod"
  },
  "dependencies": {
    "serverless-stack-output": "^0.2.3"
  },
  "devDependencies": {
    "serverless": "^4.0.0",
    "cross-env": "^7.0.3"
  }
}

